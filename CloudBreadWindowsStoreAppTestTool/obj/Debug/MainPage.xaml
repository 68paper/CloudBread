        <Button x:ConnectionId='9'  Content="CBAddUseMemberItemUPDATE" HorizontalAlignment="Left" Height="49" Margin="900,565,0,0" VerticalAlignment="Top" Width="247"                                       />
        <Button x:ConnectionId='10'  Content="CBAddUseMemberItemDELETE" HorizontalAlignment="Left" Height="49" Margin="70,633,0,0" VerticalAlignment="Top" Width="247"                                       />

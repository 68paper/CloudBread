        <TextBox x:ConnectionId='1' x:Name="outputTextbox" HorizontalAlignment="Left" Height="121" Margin="73,332,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="1210"                                  />
        <TextBlock HorizontalAlignment="Left" Height="37" Margin="73,39,0,0" TextWrapping="Wrap" Text="서버주소" VerticalAlignment="Top" Width="117"/>

        <Button x:ConnectionId='5'  Content="CBSelNotices" HorizontalAlignment="Left" Height="49" Margin="900,493,0,0" VerticalAlignment="Top" Width="247"                           />
        <Button x:ConnectionId='6'  Content="CBSelGameEvents" HorizontalAlignment="Left" Height="49" Margin="70,565,0,0" VerticalAlignment="Top" Width="247"                              />
        <Button x:ConnectionId='7'  Content="CBSelCoupons" HorizontalAlignment="Left" Height="49" Margin="342,565,0,0" VerticalAlignment="Top" Width="247"                           />
